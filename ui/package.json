{
  "name": "rest-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.5",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@popperjs/core": "^2.11.8",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "axios": "^1.9.0",
    "i18next": "^25.1.2",
    "i18next-browser-languagedetector": "^8.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "~15.5.1",
    "react-router-dom": "^7.6.0",
    "stacktrace-js": "^2.0.2",
    "typescript": "^5.9.3",
    "web-vitals": "^5.0.0"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build --base=/ui",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "/",
  "devDependencies": {
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^4.0.16",
    "nyc": "^17.1.0",
    "ts-node": "^10.9.2",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^4.0.16"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
